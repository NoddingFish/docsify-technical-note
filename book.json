{
  "title" : "title",
  "author" : "author",
  "description" : "描述",
  "language" : "zh-hans",
  "gitbook" : "3.2.3",
  "links" : {
    "sidebar" : {
      "title" : "http://www.xxxxx.com"
    }
  },
  "theme-default": {
    "showLevel": true
  },
  "plugins": [
    "expandable-chapters-small",
    "theme-comscore",
    "github",
    "search-pro",
    "-search",
    "-lunr",
    "back-to-top-button",
    "flexible-alerts",
    "include-codeblock",
    "ace",
    "tbfed-pagefooter",
    "atoc"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/NoddingFish"
    },
    "flexible-alerts": {
      "style": "flat",
      "comment": {
        "label": "Comment",
        "icon": "fa fa-comments",
        "className": "info"
      }
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": false,
      "theme": "coffee"
    },
    "tbfed-pagefooter": {
      "copyright":"Copyright &copy xxxxx.com 2019",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "atoc": {
      "addClass": true,
      "className": "atoc"
    }
  }
}